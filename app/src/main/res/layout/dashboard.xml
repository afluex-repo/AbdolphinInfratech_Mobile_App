<LinearLayout android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"

    android:orientation="vertical"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:id="@+id/container">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"


                android:orientation="vertical">
                <TextView
                    android:id="@+id/txt_heading"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="18sp"
                    android:text="Hello!"
                    android:layout_margin="40dp"

                    android:gravity="center_vertical"
                    android:layout_gravity="center_vertical"
                    android:padding="16dp"
                    android:textColor="@color/white"
                    android:fontFamily="sans-serif-black"
                    android:layout_marginEnd="16dp"/>
                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"

                    android:layout_height="wrap_content"
                    app:contentPadding="8dp"
                    android:layout_marginTop="80dp"
                    app:cardBackgroundColor="#F1F5FE"

                    android:layout_margin="5dp"
                    app:cardCornerRadius="32dp">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:layout_marginStart="16dp"
                        android:layout_marginEnd="16dp"
                        >
                        <RelativeLayout
                            android:layout_width="328dp"
                            android:layout_height="wrap_content">
                            <androidx.cardview.widget.CardView
                                android:id="@+id/card1"
                                android:layout_width="140dp"
                                android:layout_height="140dp"

                                android:outlineAmbientShadowColor="@color/black"
                                android:outlineSpotShadowColor="@color/black"
                                android:clipToPadding="false"
                                android:clipChildren="false"
                                android:layout_alignParentStart="true"
                                app:cardElevation="32dp"
                                app:contentPadding="5dp"
                                app:cardPreventCornerOverlap="false"
                                android:layout_marginStart="16dp"
                                android:layout_marginTop="16dp"
                                app:cardCornerRadius="32dp">

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:orientation="vertical">

                                    <ImageView
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:layout_weight="1"
                                        android:src="@drawable/total_associate"/>
                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:id="@+id/container1"
                                        android:padding="10dp"
                                        android:orientation="vertical"
                                        >
                                        <TextView
                                            android:layout_width="match_parent"
                                            android:layout_gravity="center"
                                            android:layout_height="wrap_content"
                                            android:gravity="center"
                                            android:textColor="@color/black"
                                            android:id="@+id/Total_Associates1"
                                            android:text="Total \n Associates"

                                            />
                                        <TextView
                                            android:layout_width="match_parent"
                                            android:layout_height="match_parent"
                                            android:gravity="center"
                                            android:textColor="#000"
                                            android:textStyle="bold"
                                            android:text="2"
                                            android:id="@+id/Total_Associates"
                                            />
                                    </LinearLayout>



                                </LinearLayout>

                            </androidx.cardview.widget.CardView>

                            <androidx.cardview.widget.CardView
                                android:layout_width="140dp"

                                android:layout_height="140dp"
                                android:layout_alignParentEnd="true"
                                android:layout_margin="16dp"


                                android:layout_marginBottom="5dp"
                                android:layout_toRightOf="@id/card1"
                                android:clipChildren="false"
                                android:clipToPadding="false"
                                android:outlineAmbientShadowColor="@color/black"
                                android:outlineSpotShadowColor="@color/black"


                                app:cardCornerRadius="32dp"
                                app:cardElevation="32dp"
                                app:cardPreventCornerOverlap="false"
                                app:contentPadding="5dp">

                                <LinearLayout
                                    android:layout_width="match_parent"

                                    android:layout_height="match_parent"
                                    android:orientation="vertical"

                                    android:weightSum="2">

                                    <ImageView
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:layout_weight="1"
                                        android:src="@drawable/total_active_id" />

                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:layout_weight="1"
                                        android:orientation="vertical">

                                        <TextView
                                            android:id="@+id/Total_Active_Id1"
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:layout_weight="1"
                                            android:gravity="center"
                                            android:text="Total \n Active Id"
                                            android:textColor="@color/black" />

                                        <TextView
                                            android:id="@+id/Total_Active_Id"
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:layout_weight="1"
                                            android:gravity="center"
                                            android:text="0"
                                            android:textColor="#000"
                                            android:textStyle="bold"

                                            />
                                    </LinearLayout>


                                </LinearLayout>
                            </androidx.cardview.widget.CardView>
                        </RelativeLayout>
                        <RelativeLayout
                            android:layout_width="328dp"
                            android:layout_height="wrap_content"

                            >
                            <androidx.cardview.widget.CardView
                                android:id="@+id/card2"
                                android:layout_width="140dp"
                                android:layout_height="140dp"
                                android:outlineAmbientShadowColor="@color/black"
                                android:outlineSpotShadowColor="@color/black"
                                android:clipToPadding="false"
                                android:clipChildren="false"



                                android:layout_alignParentStart="true"
                                app:cardElevation="32dp"
                                app:contentPadding="5dp"
                                app:cardPreventCornerOverlap="false"


                                android:layout_marginStart="16dp"
                                android:layout_marginTop="16dp"

                                app:cardCornerRadius="32dp"
                                >
                                <LinearLayout
                                    android:layout_width="match_parent"

                                    android:layout_height="match_parent"

                                    android:orientation="vertical">
                                    <ImageView
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:layout_weight="1"
                                        android:src="@drawable/team_buisness"/>
                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:layout_weight="1"
                                        android:layout_height="match_parent"
                                        android:orientation="vertical"
                                        android:background="@color/white">
                                        <TextView
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:gravity="center"
                                            android:text="Team \n Business"
                                            android:textColor="@color/black"
                                            />
                                        <TextView
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:id="@+id/Team_Business"
                                            android:gravity="center"
                                            android:textColor="#000"
                                            android:textStyle="bold"

                                            />
                                    </LinearLayout>
                                </LinearLayout>

                            </androidx.cardview.widget.CardView>


                            <androidx.cardview.widget.CardView
                                android:layout_width="140dp"

                                android:layout_height="140dp"
                                android:layout_alignParentEnd="true"
                                android:layout_margin="16dp"


                                android:layout_marginBottom="5dp"
                                android:layout_toRightOf="@id/card2"
                                android:clipChildren="false"
                                android:clipToPadding="false"
                                android:outlineAmbientShadowColor="@color/black"
                                android:outlineSpotShadowColor="@color/black"


                                app:cardCornerRadius="32dp"
                                app:cardElevation="32dp"
                                app:cardPreventCornerOverlap="false"
                                app:contentPadding="5dp">
                                <LinearLayout
                                    android:layout_width="match_parent"

                                    android:layout_height="match_parent"

                                    android:orientation="vertical">
                                    <ImageView
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:layout_weight="1"
                                        android:src="@drawable/self_buisness"/>
                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:layout_weight="1"
                                        android:orientation="vertical"
                                        android:layout_height="match_parent"
                                        android:padding="10dp">
                                        <TextView
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:textColor="@color/black"
                                            android:text="Self Business"
                                            android:gravity="center"
                                            />
                                        <TextView
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:textStyle="bold"
                                            android:gravity="center"
                                            android:textColor="#000"

                                            android:layout_marginTop="10dp"
                                            android:id="@+id/Self_Business"

                                            />

                                    </LinearLayout>
                                </LinearLayout>
                            </androidx.cardview.widget.CardView>
                        </RelativeLayout>
                        <RelativeLayout
                            android:layout_width="328dp"
                            android:layout_height="wrap_content">
                            <androidx.cardview.widget.CardView
                                android:id="@+id/card3"
                                android:layout_width="140dp"
                                android:layout_height="140dp"
                                android:outlineAmbientShadowColor="@color/black"
                                android:outlineSpotShadowColor="@color/black"
                                android:clipToPadding="false"
                                android:clipChildren="false"



                                android:layout_alignParentStart="true"
                                app:cardElevation="32dp"
                                app:contentPadding="5dp"
                                app:cardPreventCornerOverlap="false"


                                android:layout_marginStart="16dp"
                                android:layout_marginTop="16dp"

                                app:cardCornerRadius="32dp"
                                >

                                <LinearLayout
                                    android:layout_width="match_parent"

                                    android:layout_height="match_parent"

                                    android:orientation="vertical">
                                    <ImageView
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:layout_weight="1"
                                        android:src="@drawable/self_booking"/>
                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:layout_weight="1"
                                        android:layout_height="match_parent"
                                        android:orientation="vertical"
                                        android:padding="10dp"
                                        android:background="@color/white">
                                        <TextView
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:gravity="center"
                                            android:text="Self Booking"

                                            android:textColor="@color/black"

                                            />
                                        <TextView
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:layout_marginTop="10dp"
                                            android:id="@+id/Self_Booking"
                                            android:gravity="center"
                                            android:textColor="#000"
                                            android:textStyle="bold"
                                            />
                                    </LinearLayout>
                                </LinearLayout>

                            </androidx.cardview.widget.CardView>

                            <androidx.cardview.widget.CardView
                                android:layout_width="140dp"

                                android:layout_height="140dp"
                                android:layout_alignParentEnd="true"
                                android:layout_margin="16dp"


                                android:layout_marginBottom="5dp"
                                android:layout_toRightOf="@id/card3"
                                android:clipChildren="false"
                                android:clipToPadding="false"
                                android:outlineAmbientShadowColor="@color/black"
                                android:outlineSpotShadowColor="@color/black"


                                app:cardCornerRadius="32dp"
                                app:cardElevation="32dp"
                                app:cardPreventCornerOverlap="false"
                                app:contentPadding="5dp">
                                <LinearLayout
                                    android:layout_width="match_parent"

                                    android:layout_height="match_parent"

                                    android:orientation="vertical">
                                    <ImageView
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:layout_weight="1"
                                        android:src="@drawable/total_booking"

                                        />

                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:layout_weight="1"
                                        android:layout_height="match_parent"
                                        android:padding="10dp"
                                        android:orientation="vertical">
                                        <TextView
                                            android:layout_width="match_parent"
                                            android:layout_weight="1"
                                            android:layout_height="wrap_content"
                                            android:gravity="center"
                                            android:textColor="@color/black"
                                            android:text="Total Booking"
                                            android:layout_gravity="center"
                                            />
                                        <TextView
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:layout_marginTop="10dp"
                                            android:id="@+id/Total_Booking"
                                            android:gravity="center"
                                            android:textColor="#000"
                                            android:textStyle="bold"
                                            />




                                    </LinearLayout>
                                </LinearLayout>
                            </androidx.cardview.widget.CardView>
                        </RelativeLayout>
                        <RelativeLayout
                            android:layout_width="328dp"
                            android:layout_height="wrap_content">


                            <androidx.cardview.widget.CardView
                                android:id="@+id/card4"
                                android:layout_width="140dp"
                                android:layout_height="140dp"
                                android:outlineAmbientShadowColor="@color/black"
                                android:outlineSpotShadowColor="@color/black"
                                android:clipToPadding="false"
                                android:clipChildren="false"



                                android:layout_alignParentStart="true"
                                app:cardElevation="32dp"
                                app:contentPadding="5dp"
                                app:cardPreventCornerOverlap="false"


                                android:layout_marginStart="16dp"
                                android:layout_marginTop="16dp"

                                app:cardCornerRadius="32dp">
                                <LinearLayout
                                    android:layout_width="match_parent"

                                    android:layout_height="match_parent"

                                    android:orientation="vertical">
                                    <ImageView
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:layout_weight="1"
                                        android:src="@drawable/team_booking"/>

                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:layout_weight="1"
                                        android:layout_height="match_parent"
                                        android:padding="10dp"
                                        android:orientation="vertical">
                                        <TextView
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:gravity="center"
                                            android:textColor="@color/black"
                                            android:text="Team Booking"
                                            />
                                        <TextView
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:layout_marginTop="10dp"
                                            android:id="@+id/Team_Booking"
                                            android:textColor="#000"
                                            android:gravity="center"
                                            android:textStyle="bold"
                                            />

                                    </LinearLayout>
                                </LinearLayout>
                            </androidx.cardview.widget.CardView>
                            <androidx.cardview.widget.CardView
                                android:layout_width="140dp"

                                android:layout_height="140dp"
                                android:layout_alignParentEnd="true"
                                android:layout_margin="16dp"


                                android:layout_marginBottom="5dp"
                                android:layout_toRightOf="@id/card4"
                                android:clipChildren="false"
                                android:clipToPadding="false"
                                android:outlineAmbientShadowColor="@color/black"
                                android:outlineSpotShadowColor="@color/black"


                                app:cardCornerRadius="32dp"
                                app:cardElevation="32dp"
                                app:cardPreventCornerOverlap="false"
                                app:contentPadding="5dp"
                                >
                                <LinearLayout
                                    android:layout_width="match_parent"

                                    android:layout_height="match_parent"

                                    android:orientation="vertical">
                                    <ImageView
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:layout_weight="1"
                                        android:src="@drawable/total_registry"/>
                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:layout_weight="1"
                                        android:layout_height="match_parent"
                                        android:orientation="vertical"
                                        android:padding="10dp"
                                        android:background="@color/white">
                                        <TextView
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:gravity="center"
                                            android:textColor="@color/black"
                                            android:text="Total Registry"
                                            />
                                        <TextView
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:layout_marginTop="10dp"
                                            android:id="@+id/Total_Registry"
                                            android:gravity="center"
                                            android:textStyle="bold"
                                            android:textColor="#000"

                                            />
                                    </LinearLayout>
                                </LinearLayout>

                            </androidx.cardview.widget.CardView>

                        </RelativeLayout>
                        <RelativeLayout
                            android:layout_width="328dp"
                            android:layout_height="wrap_content">


                            <androidx.cardview.widget.CardView
                                android:id="@+id/card5"
                                android:layout_width="140dp"
                                android:layout_height="140dp"
                                android:outlineAmbientShadowColor="@color/black"
                                android:outlineSpotShadowColor="@color/black"
                                android:clipToPadding="false"
                                android:clipChildren="false"



                                android:layout_alignParentStart="true"
                                app:cardElevation="32dp"
                                app:contentPadding="5dp"
                                app:cardPreventCornerOverlap="false"


                                android:layout_marginStart="16dp"
                                android:layout_marginTop="16dp"

                                app:cardCornerRadius="32dp">
                                <LinearLayout
                                    android:layout_width="match_parent"

                                    android:layout_height="match_parent"

                                    android:orientation="vertical">
                                    <ImageView
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:layout_weight="1"
                                        android:src="@drawable/self_registry"/>

                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:layout_weight="1"
                                        android:layout_height="match_parent"
                                        android:orientation="vertical"
                                        android:padding="10dp">
                                        <TextView
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:gravity="center"
                                            android:textColor="@color/black"

                                            android:text="Self Registry"
                                            />
                                        <TextView
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:layout_marginTop="10dp"
                                            android:layout_gravity="center"
                                            android:gravity="center"
                                            android:textColor="#000"
                                            android:id="@+id/Self_Registry"

                                            />

                                    </LinearLayout>
                                </LinearLayout>
                            </androidx.cardview.widget.CardView>
                            <androidx.cardview.widget.CardView
                                android:layout_width="140dp"

                                android:layout_height="140dp"
                                android:layout_alignParentEnd="true"
                                android:layout_margin="16dp"


                                android:layout_marginBottom="5dp"
                                android:layout_toRightOf="@id/card5"
                                android:clipChildren="false"
                                android:clipToPadding="false"
                                android:outlineAmbientShadowColor="@color/black"
                                android:outlineSpotShadowColor="@color/black"


                                app:cardCornerRadius="32dp"
                                app:cardElevation="32dp"
                                app:cardPreventCornerOverlap="false"
                                app:contentPadding="5dp"
                                >
                                <LinearLayout
                                    android:layout_width="match_parent"

                                    android:layout_height="match_parent"

                                    android:orientation="vertical">
                                    <ImageView
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:layout_weight="1"
                                        android:src="@drawable/team_registry"/>
                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:layout_weight="1"
                                        android:layout_height="match_parent"
                                        android:background="@color/white"
                                        android:padding="10dp"
                                        android:orientation="vertical">
                                        <TextView
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:gravity="center"
                                            android:textColor="@color/black"
                                            android:text="Team Registry"
                                            />
                                        <TextView
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:layout_marginTop="10dp"
                                            android:id="@+id/Team_Registry"
                                            android:textColor="#000"
                                            android:textStyle="bold"
                                            android:gravity="center"
                                            />

                                    </LinearLayout>
                                </LinearLayout>

                            </androidx.cardview.widget.CardView>

                        </RelativeLayout>

                    </LinearLayout>

                </androidx.cardview.widget.CardView>

            </LinearLayout>





            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/white"
                android:orientation="vertical">
                <com.github.mikephil.charting.charts.PieChart
                    android:id="@+id/chart1"
                    android:layout_width="200dp"
                    android:layout_height="200dp"
                    android:layerType="hardware"
                    android:layout_gravity="center"
                    android:padding="8dp"

                    android:layout_margin="5dp" />

            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:background="@color/white"
                android:layout_marginRight="10dp"
                android:layout_marginLeft="10dp"
                >
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:textStyle="bold"
                    android:textColor="@color/colorPrimaryDark"
                    android:textSize="20dp"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:text="Current News"/>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.8dp"
                    android:background="@color/black"/>
<!--                <TextView-->
<!--                    android:layout_width="match_parent"-->
<!--                    android:layout_height="wrap_content"-->
<!--                    android:id="@+id/view_news"/>-->
            <com.smarteist.autoimageslider.SliderView
                android:id="@+id/slider"
                android:layout_width="match_parent"
                android:layout_height="200dp"
                app:sliderAnimationDuration="600"
                app:sliderAutoCycleDirection="back_and_forth"
                app:sliderIndicatorAnimationDuration="600"
                app:sliderIndicatorEnabled="true"
                app:sliderIndicatorGravity="center_horizontal|bottom"
                app:sliderIndicatorMargin="15dp"
                app:sliderIndicatorOrientation="horizontal"
                app:sliderIndicatorPadding="3dp"
                app:sliderIndicatorRadius="1.4dp"
                app:sliderIndicatorSelectedColor="#0019A6"
                app:sliderIndicatorUnselectedColor="@android:color/white"
                app:sliderScrollTimeInSec="3"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:id="@+id/container3"
                    >
                    <androidx.recyclerview.widget.RecyclerView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:padding="5dp"
                        tools:itemCount="5"
                        tools:listitem="@layout/layout_installment_single_row"
                        android:id="@+id/recyclerview"/>
                </LinearLayout>

            </LinearLayout>



        </LinearLayout>

    </androidx.core.widget.NestedScrollView>
</LinearLayout>
